<script id="check-if-logged-in" type="text/javascript">
  <% if logged_in? %>
    window.currentUser = <%= render("api/users/user.json.jbuilder", user: current_user).html_safe %>
  <% end %>
  window.keyMatch = "<%= ENV['LEMON_PARTY_TOKEN'] %>"
  window.keyMatch2 = "<%= ENV['LEMON_PARTY_TOKEN_2'] %>"
  window.keyMatch3 = "<%= ENV['LEMON_PARTY_TOKEN_3'] %>"
  window.keyMatch4 = "<%= ENV['LEMON_PARTY_TOKEN_4'] %>"
  window.showLogin = "<%= ENV['SHOW_LOGIN'] %>"
  window.CLOUDINARY_OPTIONS = {
    cloud_name: "<%= ENV['CLOUD_NAME'] %>",
    upload_preset: "<%= ENV['UPLOAD_PRESET'] %>"
  };
</script>

<main id="root"></main>
